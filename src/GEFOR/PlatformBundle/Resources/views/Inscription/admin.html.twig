{% extends "GEFORPlatformBundle::layout.html.twig" %}

{% block ocplatform_body %}

  <h2>Tableau administrateur des inscriptions</h2>

  
       
        	{% for a in entity %}
            <table>
              <tr> 
                <td>{{ a.id }}</td>           
                <td>{{ a.date|date('Y-m-d') }}</td>
                <td>{{ a.prenom }}</td>
                <td>{{ a.neele|date('Y') }}</td>
                <td>{{ a.nationalite }}</td>
                <td>{{ a.numerosecu }}</td>
                <td>{{ a.adresse }}</td>
                <td>{{ a.cp }}</td>
                <td>{{ a.formation.type }}</td>

                
                {#<td>{{ a.Formations.type }}</td> : ne fonctionne pas, A REVOIR#}
              </tr>
            </table>

            {% endfor %}<br /><br />

            {% for a in situation %}
            <table>
              <tr> 
                <td>{{ a.id }}</td>           
                <td>{{ a.type }}</td>
                <td>{{ a.finance }}</td>
              
                <td>{{ a.langue }}</td>
                <td>{{ a.informatique }}</td>
                <td>{{ a.motivation }}</td> 
                
              </tr>
            </table>

            {% endfor %}<br />
            

            {% for a in forma %}
            <table>
              <tr>
                <td>{{ a.id }}</td>                          
                <td>{{ a.type }}</td>

                  {% for b in a.candidats %}
                    <td>{{ b.prenom }}</td> 
                  {% endfor %}
                
              </tr>
            </table>

            {% endfor %}


  
     
  </ul>

{% endblock %}